  <xacro:macro name="wheel_parameters" params="reference">
    <gazebo reference="${reference}">
      <material>Gazebo/Black</material>
      <mu1 value="1"/>
      <mu2 value="1"/>
      <fdir1 value="1 1 0"/>
      <kp value="10000000.0" />
      <kd value="1.0" />
    </gazebo>
  </xacro:macro>



   <xacro:wheel_parameters reference="right_wheel_Link">
   </xacro:wheel_parameters>


   <xacro:wheel_parameters reference="left_wheel_Link">
   </xacro:wheel_parameters>


